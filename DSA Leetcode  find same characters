class Solution {
public:
    bool isValid(string s) {
     stack<char>result;
     for(char c:s)
     {
     if(c=='(' || c=='{' || c=='[')
     {
        result.push(c);
     }
     else{
      if(result.empty())
    {
        return false;
    } 
    char top=result.top();
    result.pop();
    if(c==')' && top!='(' ||
    c=='}' && top!='{'||
    c==']' && top!='['){
        return false;
    }


     }  
      
     }
     return result.empty();   
    }
};
